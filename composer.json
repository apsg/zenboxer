{
    "name": "apsg/zenboxer",
    "description": "Zenbox issues fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Szymon Gackowski",
            "email": "szymon@gackowski.edu.pl",
            "homepage": "https://gackowski.edu.pl"
        }
    ],
    "homepage": "https://github.com/apsg/zenboxer",
    "keywords": ["Laravel", "Zenboxer"],
    "require": {
        "illuminate/support": "~7|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Apsg\\Zenboxer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apsg\\Zenboxer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apsg\\Zenboxer\\ZenboxerServiceProvider"
            ]
        }
    }
}
